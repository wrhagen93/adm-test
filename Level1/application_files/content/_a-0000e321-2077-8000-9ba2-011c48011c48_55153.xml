<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e321-2077-8000-9ba2-011c48011c48_55164</versionUuid>
    <rule>
        <name>JB_showFullOCRResults</name>
        <uuid>_a-0000e321-2077-8000-9ba2-011c48011c48_55153</uuid>
        <description>Shows the full OCR result</description>
        <parentUuid>_a-0000e320-df0d-8000-9ba2-011c48011c48_53393</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!ocrResult,
  {
    #"SYSTEM_SYSRULES_ButtonArrayLayout"(
      buttons: {
        #"SYSTEM_SYSRULES_ButtonWidget"(
          label: "Perform OCR",
          saveInto: {
            #"2c242491-a05b-4e0e-a121-5966c0e22b19"(
              onSuccess: a!save(
                local!ocrResult,
                fv!result
              ),
              onError: a!save(
                local!ocrResult,
                "Error"
              )
            )
          }
        )
      }
    ),
    #"SYSTEM_SYSRULES_paragraphField"(
      readOnly: true,
      value: local!ocrResult
    )
  }
)</definition>
        <preferredEditor>interface</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
    <roleMap public="true">
        <role inherit="true" allowForAll="false" name="readers">
            <users/>
            <groups/>
        </role>
        <role inherit="true" allowForAll="false" name="authors">
